<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.dge1992.mybatis.mapper.UserMapper">

  <resultMap id="BaseResultMap" type="com.github.dge1992.mybatis.domain.User">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="user_name" jdbcType="INTEGER" property="userName" />
    <result column="age" jdbcType="VARCHAR" property="age" />
  </resultMap>

  <sql id="Base_Column_List">
    id, department_id, username, password, name, id_card_no, gender, date_of_birth, mobile,
    phone_number, login_count, nation, country, province, city, district, address, date_of_modified, 
    create_time, deleted, mobile_cornet, email, level, creator_admin_id, type
  </sql>

  <select id="selectUserList" resultType="com.github.dge1992.mybatis.domain.User">
    select * from user;
  </select>

</mapper>